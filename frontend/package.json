{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.112",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "run-p --continue-on-error --print-label lint:*",
    "lint:secretlint": "secretlint --maskSecrets --secretlintignore .gitignore '**/*'",
    "lint:prettier": "prettier --cache --check .",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "fix": "run-s --continue-on-error fix:*",
    "fix:prettier": "prettier --cache --write .",
    "fix:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@secretlint/secretlint-rule-preset-recommend": "^8.2.4",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-redos": "^4.4.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "secretlint": "^8.2.4"
  }
}
